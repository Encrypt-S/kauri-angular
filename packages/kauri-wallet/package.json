{
  "name": "kauri-wallet",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "electron-osx": "electron-packager . --platform=darwin",
    "electron-linux": "electron-packager . --platform=linux",
    "electron-win": "wine electron-packager . --platform=win32",
    "i18n": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json",
    "travis:e2e": "xvfb-run -a ng test --code-coverage --watch=false --browsers=ChromeNoSandbox",
    "travis:test": "xvfb-run -a ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "limit": "200 KB",
      "webpack": false,
      "path": "dist/kauri-wallet/"
    }
  ],
  "private": true,
  "dependencies": {
    "temp-test-module": "^1.0.0",
    "@angular/animations": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.2",
    "@angular/service-worker": "^6.0.2",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26",
    "component": "^1.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "~6.0.3",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "coveralls": "^3.0.1",
    "electron": "^2.0.2",
    "electron-packager": "^12.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-i18nsupport": "^0.16.2",
    "protractor": "~5.3.0",
    "size-limit": "^0.18.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
