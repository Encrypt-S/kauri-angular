{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  },
  {
    "name": "languages",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/ja/index.html",
        "/ja/*.css",
        "/ja/*.js",
        "/ja/*.json"
      ]
    }
  }
 ]
}